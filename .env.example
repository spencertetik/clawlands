# Claw World Environment Variables
# Copy this to .env and fill in your values

# Railway will auto-set these:
# PORT - HTTP/WebSocket port
# DATABASE_URL - PostgreSQL connection string

# Bot API Keys (comma-separated list of valid keys for bot authentication)
# Generate random keys: openssl rand -hex 16
BOT_API_KEYS=dev-key,test-key

# JWT secret for auth tokens
JWT_SECRET=change-this-in-production

# Local dev port (Railway overrides this)
PORT=3000
